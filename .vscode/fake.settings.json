{
    "ansible.python.interpreterPath": "/usr/bin/python3",
    "files.associations": {
        "**/docker-compose.yml": "dockercompose",
        "**/vars/**/*.yml": "yaml",
        "**/playbooks/*.yml": "ansible"
    },
    "yaml.schemas": {
        "https://raw.githubusercontent.com/ansible/ansible-lint/main/src/ansiblelint/schemas/vars.json": "**/playbooks/group_vars/**/*.yml"
    }
}
